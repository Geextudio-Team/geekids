{
  "license": "GPL-3.0",
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^5.0.19",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "umi": "^3.2.12"
  },
  "devDependencies": {
    "@umijs/preset-react": "^1.5.19",
    "@umijs/test": "^3.2.10",
    "lint-staged": "^10.2.11",
    "prettier": "^1.19.1",
    "yorkie": "^2.0.0"
  }
}
